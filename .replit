language = "nodejs"
run = "npx expo start --web --port 5000 --host 0.0.0.0"
modules = ["nodejs-20"]

[deployment]
run = ["sh", "-c", "node server.js"]
deploymentTarget = "cloudrun"
build = ["sh", "-c", "npm install && npx expo export --platform web"]
publicDir = "dist"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 5000

[[ports]]
localPort = 8000
externalPort = 8000

[[ports]]
localPort = 8080
externalPort = 8080

[[ports]]
localPort = 8081
externalPort = 8081

[[ports]]
localPort = 8082
externalPort = 3001

[[ports]]
localPort = 8083
externalPort = 3003

[[ports]]
localPort = 19006
externalPort = 3000

[[ports]]
localPort = 19007
externalPort = 3002

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --web --port 5000 --host 0.0.0.0"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Expo Web"

[[workflows.workflow]]
name = "Expo Web"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --web --port 5000 --host 0.0.0.0"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[agent]
integrations = ["perplexity_v0:1.0.0", "python_anthropic:1.0.0"]
